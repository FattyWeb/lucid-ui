{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "semicolon": false,
    "trailing-comma": false,
    "no-console": false, // eliminar antes de publicar
    "object-literal-sort-keys":false,
    "member-access": false,
    "triple-equals": false,
    "max-classes-per-file": [true, 2],
    "interface-name": [
      true,
      "never-prefix"
    ]
  }
}
